    <transport_descriptor>
      <transport_id>udp_transport</transport_id>
      <type>UDPv4</type>
      <interfaceWhiteList>
        <!-- IP de la Nano -->
        <address>192.168.55.1</address>
        <!-- IP del PC -->
        <address>192.168.137.196</address>
